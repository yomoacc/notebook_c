#include <stdio.h>
#define MOD 998244353

// 快速幂计算 a^b mod MOD
long long quick_pow(long long a, long long b) {
    long long result = 1;
    long long base = a % MOD; // 先对a取模，避免大数溢出
    
    while (b > 0) {
        // 若当前指数的二进制最低位为1，将结果乘上当前底数
        if (b & 1) { // 等价于 b%2 == 1
            result = (result * base) % MOD;
        }
        // 底数平方（对应指数右移一位）
        base = (base * base) % MOD;
        // 指数右移一位（等价于除以2）
        b >>= 1;
    }
    
    // 处理特殊边界：0^0（题目中a,b均为0时，结果按0处理）
    if (a == 0 && b == 0) {
        return 0;
    }
    return result;
}

int main() {
    long long a, b;
    scanf("%lld %lld", &a, &b);
    printf("%lld\n", quick_pow(a, b));
    return 0;
}
